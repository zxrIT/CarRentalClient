.container {
  min-height: 100vh;
  background: #fff;
  padding-bottom: 40rpx;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.date {
  font-size: 28rpx;
  color: #333;
}

.history-link {
  font-size: 26rpx;
  color: #999;
}

.content {
  padding: 30rpx;
}

.parties {
  margin-bottom: 40rpx;
}

.party {
  font-size: 28rpx;
  color: #333;
  line-height: 1.8;
}

.agreement-text {
  font-size: 28rpx;
  color: #333;
  line-height: 1.8;
  margin-bottom: 40rpx;
}

.section {
  margin-bottom: 30rpx;
}

.section-title {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.section-content {
  padding-left: 20rpx;
}

.clause {
  font-size: 28rpx;
  color: #333;
  line-height: 1.8;
  margin-bottom: 16rpx;
} 